<% p("logstash.pipelines", []).each do |pipeline| %>
- pipeline.id: <%= pipeline["name"] %>
  path.config: "/var/vcap/jobs/logstash/config/conf.d/<%= pipeline['name'] %>/*.conf"
<% if !pipeline["params"].empty? %><% pipeline["params"].each do | k , v | %>  <%= k %> : <%= v %>
<% end %>
<% end %>
<% end %>
